            <material name="blue"/>
        </visual>
		<collision>
			<geometry>
				<box size="1 1 0.05"/>
			</geometry>
			<contact_coefficients mu="0.01" kp="1" kd="0.01"/>
		</collision>
		<inertial>
			<mass value="3.0"/>
			<inertia ixx="0.00833" ixy="0" ixz="0" iyy="0.00333" iyz="0" izz="0.00833"/>
		</inertial>
	</link>
</robot>
